{
  "name": "repolisher",
  "version": "0.1.0",
  "author": "Robert Mrowiec @RobertMrowiec",
  "bin": {
    "repolisher": "./bin/run"
  },
  "bugs": "https://github.com/Selleo/repolisher/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.0.1",
    "@octokit/request": "^5.4.5",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "^6.5.0",
    "colors": "^1.4.0",
    "diff": "^4.0.2",
    "glob": "^7.1.6",
    "inquirer": "^7.1.0",
    "path": "^0.12.7",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/diff": "^4.0.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.24",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "chai": "^4.2.0",
    "eslint": "^7.3.1",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "globby": "^10.0.2",
    "husky": "^4.3.0",
    "mocha": "^7.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/src/files/*",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Selleo/repolisher",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "repolisher",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": "Selleo/repolisher",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
